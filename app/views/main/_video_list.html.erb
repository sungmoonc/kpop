
<a href="https://www.youtube.com/embed/{{youtube_id}}" data-toggle="modal" data-target="#basicModal" class="hook--showmodal">
	<div class="dataWrapper"
		data-title="{{title_korean}}" data-youtube-userid="{{youtube_user_id}}" data-uploaded="{{upload_date}}" data-category="{{category}}"
		data-views="{{youtube_views}}" data-upvotes="{{upvotes}}" data-downvotes="{{downvotes}}"
		data-approval="{{approval_rating}}" data-hotness="{{hotness}}" data-cheese="{{cheesiness}}">
	</div>
	<div class="box panel panel-default">
		<img src="{{thumbnail}}">		
		<div class="titleKr">{{title_korean}}</div>
		<div class="overlay">
			<div class="song_title">{{title_korean}}</div>			
		</div>
	</div>
	<div class="list-info">	  
      	<div class="titleD">{{title_korean}}</div>              
      	<div class="row">        	
          <div class="youtube_user_id">{{youtube_user_id}}</div>
          <div class="category">{{category}}</div>
          <div class="upload_date col-md-2">{{upload_date}}</div>
        </div>
        <div class="row">        	
	        <div class="views col-md-2">{{youtube_views}}</div>
	        <div class="upvotes col-md-2">{{upvotes}}</div>
	        <div class="downvotes col-md-2">{{downvotes}}</div>
	        <div class="approval col-md-2">{{approval_rating}}</div>
        </div>
        <div class="row">
        	<div class="hotness col-md-4">{{hotness}}</div>
        	<div class="cheesiness col-md-4">{{cheesiness}}</div>
        </div>
    </div>      
</a>